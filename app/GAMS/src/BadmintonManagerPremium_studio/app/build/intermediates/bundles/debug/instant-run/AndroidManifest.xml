<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.gr.java_conf.bunooboi.bmp"
    android:versionCode="12"
    android:versionName="1.3.3" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Start"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateAlwaysHidden" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Config"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Main"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.History"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Download"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Overview"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.ScoreTable"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Larry"
            android:launchMode="singleTask" />
        <activity
            android:name="jp.gr.java_conf.bunooboi.bmp.Gallery"
            android:launchMode="singleTask" />
        -
        <receiver android:name="jp.gr.java_conf.bunooboi.bmp.Broadcast" >
            -
            <intent-filter>
                <action android:name="android.intent.action.DOWNLOAD_COMPLETE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>
    </application>

</manifest>